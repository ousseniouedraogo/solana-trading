# MongoDB
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/copyTradingBot

# Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_ADMIN_ID=your_telegram_user_id
ADMIN_CHAT_ID=your_admin_chat_id

# Solana Configuration
SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_API_KEY
SOLANA_WSS_URL=wss://mainnet.helius-rpc.com/?api-key=YOUR_API_KEY
SOLANA_PRIVATE_KEY=your_base58_encoded_private_key

# API Keys
MORALIS_API_KEY=your_moralis_api_key
INCH_API_KEY=your_1inch_api_key

# Polling Frequencies (in milliseconds)
SWAP_PROCESSING_FREQ=30000
NEW_SWAP_POLLING_FREQ=60000
POSITION_MONITOR_FREQ=30000
MINT_DETECTION_FREQ=10000

# Auto-Snipe Configuration
AUTO_SNIPE_TRACKED=true
AUTO_SNIPE_AMOUNT=0.01
AUTO_SNIPE_TP=75
AUTO_SNIPE_SL=20
MAX_SNIPE_AGE_SECONDS=300

# Market Cap Filter (Option 1: Wide Range for Testing)
AUTO_SNIPE_MCAP_FILTER=true
AUTO_SNIPE_TARGET_MCAP_MIN=1000
AUTO_SNIPE_TARGET_MCAP_MAX=50000

# Fast Execution Settings
FAST_SNIPE_ENABLED=true
FAST_SNIPE_SKIP_PREFLIGHT=true
FAST_SNIPE_MAX_RETRIES=3

# Priority Fee Settings (in microlamports)
PRIORITY_FEE_MIN=1000
PRIORITY_FEE_MAX=100000
PRIORITY_FEE_PERCENTILE=90
